{
  "name": "matthiasmax.com",
  "private": true,
  "version": "0.0.1",
  "description": "Personal developer website of Matthias Max",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "prod": "stencil build --prod --watch --serve",
    "static": "stencil build --prod --prerender",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watch",
    "generate": "stencil generate",
    "icons": "pwa-asset-generator src/assets/logo.svg src/assets/icons -m src/manifest.json -f -i src/index.html"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@ionic/core": "^4.11.5"
  },
  "devDependencies": {
    "@stencil/core": "^2.0.3",
    "@stencil/sass": "^1.3.2",
    "pwa-asset-generator": "^2.1.2",
    "workbox-build": "4.3.1"
  }
}
